     public static Category getCategory(String categoryName){
         Category category = null;
         switch (categoryName){
             case "MERCH":
                 category = Category.MERCH;
                 break;
             case "STATIONERY":
                 category = Category.STATIONERY;
                 break;
             case "CLOTHES":
                 category = Category.CLOTHES;
